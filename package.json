{
  "name": "vite-plugin-istanbul",
  "version": "0.0.0",
  "author": "iFaxity (christian@faxity.se)",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "homepage": "https://github.com/ifaxity/vite-plugin-istanbul",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifaxity/vite-plugin-istanbul.git"
  },
  "bugs": {
    "url": "https://github.com/ifaxity/vite-plugin-istanbul/issues"
  },
  "keywords": [
    "vite",
    "babel",
    "plugin",
    "istanbul",
    "nyc"
  ],
  "dependencies": {
    "@istanbuljs/load-nyc-config": "^1.1.0",
    "espree": "^9.6.1",
    "istanbul-lib-instrument": "^6.0.1",
    "picocolors": "^1.0.0",
    "test-exclude": "^6.0.0"
  },
  "peerDependencies": {
    "vite": ">=2.9.1 <= 5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.11.17",
    "@types/ws": "^8.5.10",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
}
