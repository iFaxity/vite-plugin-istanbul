{
  "$schema": "https://json.schemastore.org/prettierrc",
  "trailingComma": "es5",
  "singleQuote": true,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "proseWrap": "preserve",
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "semi": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "",
    "^[./]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
